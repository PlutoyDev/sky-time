    "@yarnpkg/pnpify": "^4.0.0-rc.3",
    "jest": "^27.5.1",
    "ntl": "^5.1.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@luvies/lazy": "^1.7.2",
    "@types/node": "^17.0.29",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.3",
    "mongoose": "^6.3.1",
    "tslib": "^2.4.0"
  }
}
