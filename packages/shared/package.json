{
  "name": "@sky-time/shared",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "typings": "./src/index.ts",
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w --preserveWatchOutput",
    "get-deps": "cat package.json | jq ' .dependencies | keys | join(\" \") ' | tr -d '\\\"' ",
    "sync-client": "yarn workspace @sky-time/client add -D $( yarn get-deps ) "
  },
  "devDependencies": {
    "ntl": "^5.1.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@types/node": "^17.0.25",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.3",
    "mongoose": "6.3.0",
    "mongoose-autopopulate": "^0.16.1",
    "tslib": "^2.4.0",
    "typegoose": "^5.9.1"
  }
}
